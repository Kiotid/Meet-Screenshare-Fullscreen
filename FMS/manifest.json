{
    "name": "Fullscreen-Meet-Screenshare",
    "version": "1.0.0",
    "description": "Now u can zoom and maximize the screenshare on Google Meet",
    "manifest_version": 3,
    "author": "Samuele Radici",
    "content_scripts":[
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["inject.js"]
        }
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Fullscreen-Meet-Screenshare"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ]
}
